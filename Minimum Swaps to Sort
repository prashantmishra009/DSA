	int minSwaps(vector<int>&nums)
	{
	    vector<int>temp=nums;
	    int n=nums.size(),count=0;
	    
	    sort(temp.begin(),temp.end());  
	    
	    unordered_map<int,int>map;    
	    for(int i=0;i<n;i++){
	        map[nums[i]]=i;
	    }
	    
	
	    
	    for(int i=0;i<n;i++){
	        if(nums[i]!=temp[i]){       
	            swap(nums[i],nums[map[temp[i]]]);
	            swap(map[nums[i]],map[nums[map[temp[i]]]]);
	            count++;
	        }
	    }
	    
	    return count;
	}
