vector<int> find(int arr[], int n , int key )
{
    // code here
     vector<int> ans;
    int first=-1;
    int last=-1;
    for(int i=0;i<n;i++){
        if(arr[i]==key){
            if(first==-1 && last==-1){
                first=i;
                last=i;
            }
            else{
                last=i;
            }
        }
        
        
    }
    ans.push_back(first);
        ans.push_back(last);
        return ans;
}
