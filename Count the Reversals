int countRev (string s)
{
    // your code here
      int n=s.length();
    int ans=0;
    if(n%2 == 1) return -1;
    int open=0;
    int close=0;
    for(int i=0;i<n;i++){
        if(s[i]=='{') open++;
        else{
            if(open==0){
                ans++;
                open++;
            }
            else{
                open--;
            }
        }
       // if(s[i]=='}') close++;
    }
   ans = ans+((open)/2);
   return ans;
}
