 string PartyType(long long int a[], int n)
    {
        // Your code goes here
        unordered_map<int,int> myMap;
        for(int i=0;i<n;i++){
           

       myMap[a[i]]++;
    
        }
        bool check=0;//girls;
         for (auto i = myMap.begin(); i!=myMap.end(); ++i) {
       if(i->second>=2) {
           check=1;
           break;
       }
    }
    if(check==1) return "BOYS";
    if(check==0) return "GIRLS";
    
        
    }
