 int missingNumber(vector<int>& array, int n) {
        // Your code goes here
          int sum = 0;/////  made mistake by getting confused in size and given n;
        
        for(int i=0; i<array.size(); i++)
        {
            sum += array[i];
        }
        
        int sumTotal = n*(n+1)/2;
        
        return sumTotal-sum;
    }
